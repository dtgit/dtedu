      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone">

<body>

<!-- THE PORTAL LOGO DEFINITION -->
<a metal:define-macro="portal_logo"
   id="portal-logo"
   accesskey="1"
