<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
   cookie_persistence="False" default_skin="Plone Default"
   request_varname="plone_skin">
  <object name="ECAssignmentBox" meta_type="Filesystem Directory View"
	  directory="ECAssignmentBox/skins/ECAssignmentBox"/> 
  <object name="ECAssignmentBox_public" meta_type="Filesystem Directory View"
	  directory="ECAssignmentBox/skins/ECAssignmentBox_public"/>

 <skin-path name="*">
  <layer name="ECAssignmentBox_public" insert-after="custom"/>
  <layer name="ECAssignmentBox" insert-after="custom"/>
 </skin-path>
</object>
